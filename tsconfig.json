{
  "include": ["./src/**/*"],
  "compilerOptions": {
    "module": "ES2020",
    "target": "ES2020",
    "moduleResolution": "node",
    "esModuleInterop": true,
    "paths": {
      "@googlemaps/marker": ["./src/index.ts"]
    },
    "noEmit": true,
    "strict": true,
    "skipLibCheck": true,
    "sourceMap": true,
    "inlineSourceMap": true
  }
}
